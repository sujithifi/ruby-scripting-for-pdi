	<scripts>
		<script>
			<title>Ruby Script</title>
			<body># This script aggregates the total goals scored during a game of 4 quarters
# The rows in the input come in groups of four
# The first row has already been passed into the script

game = [$row] + $input.read(3)

total_goals = game.reduce(0) {|sum, row| sum + row[&quot;goals&quot;]}

# evaluate to the game id and total goals
{
	&quot;game_id&quot; =&gt; $row[&quot;game_id&quot;],
	&quot;total_goals&quot; =&gt; total_goals
}</body>
			<role>ROW_SCRIPT</role>
		</script>
	</scripts>
	<rubyVersion>RUBY_1_8</rubyVersion>
	<clearInputFields>Y</clearInputFields>

