         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
	<scripts>
		<script>
			<title>Ruby Script</title>
			<body>####################################################################################
# Ruby Scripting Step for Kettle 
# Created by Slawomir Chodnicki 
# http:&#47;&#47;type-exit.org
####################################################################################
# The following methods are useful for dealing with Kettle variables
#
# $step.list_variables gets an array of defined variables
# $step.get_variable(var) gets the value of a variable
# $step.environment_substitute(str) replaces all occurrences of any variable in str
#
# This script prints the Kettle version to log and continues to 
# generate a row for every Kettle variable defined.
#

# write something to log
$step.log_basic(&quot;*************************************************************&quot;)
$step.log_basic($step.environment_substitute(&quot;You are running Kettle version: ${Internal.Kettle.Version}&quot;))
$step.log_basic(&quot;*************************************************************&quot;)

# accumulate all variables as rows with &quot;var&quot; and &quot;value&quot; fields
rows = []

$step.list_variables.sort.each do |name|
	rows &lt;&lt; {&quot;var&quot; =&gt; name, &quot;value&quot; =&gt; $step.get_variable(name)}
end

# evaluate to the accumulated rows 
rows</body>
			<role>ROW_SCRIPT</role>
		</script>
	</scripts>
