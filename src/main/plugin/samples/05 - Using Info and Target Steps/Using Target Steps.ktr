	<scripts>
		<script>
			<title>Ruby Script</title>
			<body>########################################################################
# Ruby Scripting Step for Kettle 
# Created by Slawomir Chodnicki 
# http:&#47;&#47;type-exit.org
########################################################################
# Using target steps
#
# this script looks at the incoming row and sends rows with even numbers
# and rows with uneven numbers to different target steps
#

if $row[&quot;num&quot;].even? then
	$target_steps[&quot;even&quot;] &lt;&lt; $row
else
	$target_steps[&quot;uneven&quot;] &lt;&lt; $row
end

# Sometimes you may want to write to all output steps at once:
# a call to $output.write will write to *all* target steps at once.
# This sample sends a row containing -1 to *both* target steps if this is 
# the last row processed

$output &lt;&lt; {&quot;num&quot; =&gt; -1} if $row[&quot;is_last&quot;]
</body>
			<role>ROW_SCRIPT</role>
		</script>
