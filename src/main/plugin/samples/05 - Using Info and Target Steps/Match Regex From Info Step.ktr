  <step>
    <name>Pass matching strings</name>
    <type>RubyStep</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
	<scripts>
		<script>
			<title>Ruby Script</title>
			<body># this script passes only rows which match
# at any of the regexes produced by the info step

# initialize by reading the regexes from the info step into an array
regex_list ||= $info_steps[&quot;regex&quot;].read_all.map{|x| x[&quot;regex&quot;]}

# check if the current row matches one of the regexes
matched_by = regex_list.find{|regex| &#47;#{regex}&#47; =~ $row[&quot;str&quot;]}

# if a match is found, add the matching regex and pass on the row
if !matched_by.nil? then
	$row[&quot;matched_by&quot;] = matched_by
	$row
else
# if there&apos;s no match, evaluate to nil, so no row is written
	nil
end</body>
			<role>ROW_SCRIPT</role>
		</script>
	</scripts>
	<rubyVersion>RUBY_1_8</rubyVersion>
